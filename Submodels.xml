<PropertyList>
 <!-- GSh-23L gun -->
 <submodel>
  <name>gun-left1</name>
  <model>Aircraft/j22/Models/Effects/Gun/Tracer.xml</model>
  <trigger>controls/armament/trigger</trigger>
  <speed>3380</speed>
  <repeat>true</repeat>
  <delay>0.05</delay>
  <count>-1</count>
  <x-offset>-3.75</x-offset>
  <y-offset>-0.60</y-offset>
  <z-offset>-0.61</z-offset>
  <yaw-offset>0</yaw-offset>
  <pitch-offset>0</pitch-offset>
  <life>15</life>
  <wind>false</wind>
  <weight>0.2249</weight>
  <cd>0.075</cd>
  <eda>0.01051198</eda>
  <collision>true</collision>
  <collision-report>sim/ai/aircraft/collision/gun</collision-report>
  <impact>true</impact>
  <impact-report>sim/ai/aircraft/impact/bullet</impact-report>
  <submodel-path>Aircraft/j22/Models/Effects/Gun/Bullet-Submodel.xml</submodel-path>
 </submodel>
